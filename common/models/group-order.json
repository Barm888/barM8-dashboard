    "instruction": {
      "type": "string",
      "default": null
    },
    "totalAmt": {
      "type": "number",
      "default": 0
    },
    "orderTotal": {
      "type": "number",
      "default": 0
    },
    "subTotal": {
      "type": "number",
      "default": 0
    },
    "serviceFee": {
      "type": "number",
      "default": 0
    },
    "chargeAmt": {
      "type": "number",
      "default": 0
    },
    "transActionFeeType": {
      "type": "string"
    },
    "isReservation": {
      "type": "boolean",
      "default": false
    },
    "payingTogether": {
      "type": "boolean",
      "default": false
    },
    "payingSeparately": {
      "type": "boolean",
      "default": false
    },
    "paymentDescription": {
      "type": "string",
      "default": null
    },
    "paymentStatus": {
      "type": "string",
      "default": "Not Paid"
    },
    "token": {
      "type": "string",
      "default": null
    },
    "confirm": {
      "type": {
        "countryFormat": {
          "type": "date"
        },
        "date": {
          "type": "string",
          "default": null
        },
        "time": {
          "type": "string",
          "default": null
        }
      }
    },
    "isOrdered": {
      "type": "boolean",
      "default": false
    },
    "isInviter": {
      "type": "boolean",
      "default": false
    },
    "isCreate": {
      "type": "date",
      "default": "$now"
    }
