      "type": "belongsTo",
      "model": "Order",
      "foreignKey": ""
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    },
    "orderLines": {
      "type": "hasMany",
      "model": "OrderLine",
      "foreignKey": ""
    },
    "paymentHistories": {
      "type": "hasMany",
      "model": "PaymentHistory",
      "foreignKey": ""
    }
