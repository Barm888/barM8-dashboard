{
  "name": "Category",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "imageUrl": {
      "type": "string"
    },
    "isCreate": {
      "type": "date",
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "business": {
      "type": "belongsTo",
      "model": "Business",
      "foreignKey": "ownerId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
