{
  "name": "Booking",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date"
    },
    "bId": {
      "type": "string",
      "require": true
    },
    "totalQty": {
      "type": "number",
      "default": 0
    },
    "status": {
      "type": "string"
    },
    "paymentStatus": {
      "type": "string"
    },
    "totalPrice": {
      "type": "number",
      "default": 0
    },
    "currency": {
      "type": "string",
      "default": "AUD"
    },
    "isCreate": {
      "type": "date",
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    },
    "dashLine": {
      "type": "belongsTo",
      "model": "DashLine",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
