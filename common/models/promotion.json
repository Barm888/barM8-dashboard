{
  "name": "Promotion",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "status": {
      "type": "string",
      "default": "Pending"
    },
    "frequency": {
      "type": "number"
    },
    "sponsorName": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "desc": {
      "type": "string"
    },
    "startDate": {
      "type": "date"
    },
    "sDate": {
      "type": "any",
      "default": {
        "dayNo": "number",
        "month": "number",
        "year": "number",
        "date": "string"
      }
    },
    "endDate": {
      "type": "date"
    },
    "eDate": {
      "type": "any",
      "default": {
        "dayNo": "number",
        "month": "number",
        "year": "number",
        "date": "string"
      }
    },
    "startTime": {
      "type": "string"
    },
    "endTime": {
      "type": "string"
    },
    "isLive": {
      "type": "boolean",
      "default": false
    },
    "liveDate": {
      "type": "date"
    },
    "liveTime": {
      "type": "string"
    },
    "live": {
      "type": "any"
    },
    "isImage": {
      "type": "boolean",
      "default": false
    },
    "primaryImg": {
      "type": [
        "object"
      ]
    },
    "secondaryImg": {
      "type": [
        "object"
      ]
    },
    "video": {
      "type": [
        "object"
      ]
    },
    "videoThumbnail": {
      "type": [
        "object"
      ]
    },
    "isCreate": {
      "type": "date",
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "sponsorDetails": {
      "type": "belongsTo",
      "model": "SponsorDetails",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
