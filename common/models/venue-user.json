{
  "name": "VenueUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "phoneNo": {
      "type": "number"
    },
    "pin": {
      "type": "number"
    },
    "isAdmin": {
      "type": "boolean",
      "default": false
    },
    "status": {
      "type": "string",
      "default": "Pending"
    },
    "userType": {
      "type": "string"
    },
    "isCreated": {
      "type": "date",
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "business": {
      "type": "belongsTo",
      "model": "Business",
      "foreignKey": "ownerId"
    },
    "venuePermissions": {
      "type": "hasMany",
      "model": "VenuePermission",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
