{
  "name": "Visit",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "visitDate": {
      "type": "date"
    },
    "scanFrequency": {
      "type": "number",
      "default": 1
    },
    "scanArray": {
      "type": [
        "date"
      ],
      "default": []
    },
    "lastScanTime": {
      "type": "date"
    },
    "startTime": {
      "type": "date"
    },
    "endTime": {
      "type": "date"
    },
    "location": {
      "type": "geopoint"
    },
    "id1": {
      "type": "string",
      "id": true,
      "defaultFn": "uuid"
    },
    "createTs": {
      "type": "date"
    },
    "createProgId": {
      "type": "string"
    },
    "modifyTs": {
      "type": "date"
    },
    "modifyProgId": {
      "type": "string"
    },
    "isCreate": {
      "type": "date",
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "scans": {
      "type": "embedsMany",
      "model": "Scan",
      "property": "scanse",
      "options": {
        "validate": true,
        "forceId": true
      }
    },
    "business": {
      "type": "belongsTo",
      "model": "Business",
      "foreignKey": "ownerId"
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
